# 1. Create
my_list = []

# 2. Append
my_list.append(10)
my_list.append(20)
my_list.append(30)
my_list.append(40)


# 3. Insert
my_list.insert(1, 15)  # Index 1 is the second position

# 4. Extend 
my_list.extend([50, 60, 70])

# 5. Remove
my_list.pop()  # Removes 70


# 6. Sort in ascending order
my_list.sort()

# 7. Find and print 
print(my_list.index(30))
